<div class="container gallery">
  <h1 class="page-title">Photo Gallery</h1>
  {{> instagram-button}}
  <div class="instagram-feed">Loading Instagram Feed...</div>

  <div class="blueimp-gallery blueimp-gallery-controls blueimp-gallery-full-page">
    <div class="slides"></div>
    <h3 class="title"></h3>
    <a class="prev">‹</a>
    <a class="next">›</a>
    <a class="close">×</a>
    <a class="play-pause"></a>
    <ol class="indicator"></ol>
  </div>
</div>
